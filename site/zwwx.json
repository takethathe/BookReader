{
    'name': 'zwwx',
    'getItem': function(data, book_info, addItem) {
        var data_ = $(data);
        $('dd a', data_).each(function(i, item){
            var link = 'http://www.zwwx.com/book/3/'+book_info['id']+'/'+$(item).attr('href');
            var title = $(item).text();
            addItem({
                'link':link,
                'title':title,
            });
        });
    }
}